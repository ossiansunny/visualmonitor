host=$1
os=$2
comm=$3
plotpath="/var/www/html/plot"
snmppath="/var/www/html/bin"
cpu=`${snmppath}/snmpcpuget $host $os $comm`
ram=`${snmppath}/snmpramget $host $os $comm`
disk=`${snmppath}/snmpdiskget $host $os $comm`
cpuv=`echo $cpu | awk '{print $1}'`
ramv=`echo $ram | awk '{print $1}'`
diskv=`echo $disk | awk '{print $1}'`
dte=`date +"%H"`
echo "$dte $cpuv $ramv $diskv" > $plotpath/plotimage/$host.tmp
cat ${plotpath}/plotimage/${host}.tmp >> ${plotpath}/plotimage/${host}.log
